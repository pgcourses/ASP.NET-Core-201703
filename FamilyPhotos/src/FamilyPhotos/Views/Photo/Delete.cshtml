@model FamilyPhotos.ViewModel.PhotoViewModel

<div class="page-header">
    <h2>Kép törlés</h2>
</div>


@using (Html.BeginForm(FormMethod.Post,htmlAttributes: new { @class = "form-horizontal" }))
{
    @Html.HiddenFor(m => m.Id)
    @Html.DisplayForModel()
    <div class="col-md-offset-2">
        @Html.ActionLink("Vissza a képek listájára", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
        <input type="submit" value="Törlés"  class="btn btn-danger"/>
    </div>
}

